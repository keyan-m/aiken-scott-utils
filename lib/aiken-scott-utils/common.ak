pub type Scott2<a, b, result> =
  fn(a, b) -> result

pub type Scott3<a, b, c, result> =
  fn(a, b, c) -> result

pub type Scott4<a, b, c, d, result> =
  fn(a, b, c, d) -> result

pub type Scott5<a, b, c, d, e, result> =
  fn(a, b, c, d, e) -> result

pub type Scott6<a, b, c, d, e, f, result> =
  fn(a, b, c, d, e, f) -> result
